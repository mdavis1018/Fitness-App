<h1>Workout Details</h1>
<%= form_with model: @workout_detail do |form| %>
  <div>
    <%= form.label :reps %>
    <%= form.text_field :reps %>
  </div>
  <div>
    <%= form.label :weight %>
    <%= form.text_field :weight %>
  </div>
    <%= select_tag(:workout_id, options_for_select(current_user.workouts.map do |workout|
    [workout.title, workout.id]
    end
    )) %>

  <div>
    <%= form.submit %>
  </div>
<% end%>


