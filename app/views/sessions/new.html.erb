<div style="color:#FFFFFF">
<div style="text-align:center">
<h1>Login</h1>
<%= form_with method: "post", url: "/sessions" do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Login", class: "btn btn-outline-light"  %>
  </div>
  </div>
<% end%>
